{
  "less": {
    "dest": "public",
    "render": {
      "compress": false
    }
  },
  "js": {
    "dest": "public",
    "src": "assets/js",
    "files": {
      "/js/vendor.js": {
        "require": {
          "angular": "angular",
          "angular-resource": "angular-resource",
          "angular-bootstrap": "angular-bootstrap",
          "angular-ui-router": "angular-ui-router",
          "lodash": "lodash",
          "jquery": "jquery"
        }
      },
      "/js/app.js": {
        "files": [
          "app.js"
        ],
        "external": [
          "angular",
          "angular-resource",
          "angular-bootstrap",
          "angular-ui-router",
          "lodash",
          "jquery"
        ]
      }
    }
  }
}
